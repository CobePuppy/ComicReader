{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ComicReader",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/ComicReader.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "D:/Qt/6.9.1/mingw_64/bin;D:/Qt/Tools/mingw1310_64/bin;${env:PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "D:/Qt/Tools/mingw1310_64/bin/gdb.exe",
            "preLaunchTask": "Build ComicReader",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
